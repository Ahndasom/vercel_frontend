{
  "version": 2,
  "functions": {
    "api/index.py": {
      "runtime": "python@3.11",
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ],
  "env": {
    "BACKEND_URL": "https://charissa-reviewable-pseudoimpartially.ngrok-free.dev"
  }
}